

<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Suite2" verbose="1">
    <listeners>
        <listener class-name="TestNg.Listeners" />
    </listeners>

    <parameter name="NAME" value="Navneet Ranjan" />

    <test name="package Testing">
        <parameter name="NAME" value="Saurabh Kumar" />
        <packages>
            <package name="TestNg" />
        </packages>
    </test>

    <test name="Before and After Methods" >
        <classes>
            <class name="TestNg.Test1" />
            <class name="TestNg.Basics" />
        </classes>

    </test>

    <test name="Group functionality">
        <parameter name="NAME" value="KUMAR NAVNEET" />
        <groups>
            <run>
                <include name="Navneet" />
                <include name="Ranjan" />
            </run>
        </groups>
        <packages>
            <package name="TestNg" />
        </packages>
    </test>
</suite>
